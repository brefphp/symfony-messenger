{
    "name": "bref/symfony-messenger",
    "description": "Symfony Messenger bridge to run with SQS and SNS on AWS Lambda with Bref",
    "keywords": ["bref", "symfony", "messenger", "sqs", "sns", "aws", "aws-lambda"],
    "license": "MIT",
    "type": "library",
    "autoload": {
        "psr-4": {
            "Bref\\Symfony\\Messenger\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bref\\Symfony\\Messenger\\Test\\": "tests/"
        }
    },
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "symfony/messenger": "^4.3 || ^5.0",
        "symfony/config": "^4.3 || ^5.0",
        "symfony/dependency-injection": "^4.3 || ^5.0",
        "symfony/http-kernel": "^4.3 || ^5.0",
        "symfony/yaml": "^4.3 || ^5.0",
        "bref/bref": "^0.5.18 || ^1.0",
        "async-aws/sns": "^1.0",
        "async-aws/sqs": "^1.2",
        "async-aws/event-bridge": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.4",
        "mnapoli/hard-mode": "^0.3.0",
        "phpstan/phpstan": "^0.12.0",
        "symfony/framework-bundle": "^4.3 || ^5.0",
        "nyholm/symfony-bundle-test": "dev-master",
        "matthiasnoback/symfony-dependency-injection-test": "^4.1"
    }
}
